<div class="videos-container">
  <header id="video-header">
    <ul class="video-header-mid no-mobile">
      <% @tags.each do |tag| %>
        <li class="video-header-item">
          <p class="<%= tag.name %>">
            <%= tag.name %>
          </p>
        </li>
      <% end %>
       <li class="video-header-item active">
        <p class="ALL">
          ALL
        </p>
      </li>
    </ul>
  </header>
  <div class="section-content">
    <h2 class="section-title">FEATURED</h2>
    <%= render partial: "featured_videos" %>
    <h2 class="section-title">QUICK WATCHES</h2>
    <ul class="regular-videos">
      <% @videos.each do |video| %>
        <%= render partial: 'video_item_container', locals: {video: video} %>
      <% end %>
    </ul>
    <%= render partial: 'layouts/footer' %>
  </div>
</div>
